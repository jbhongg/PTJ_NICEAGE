eMyId')\r\n      store.commit('root/deleteToken')\r\n      router.push({\r\n        name: 'Home'\r\n      })\r\n    }\r\n\r\n    const clickAdmin = () => {\r\n      router.push({\r\n        name: 'Admin'\r\n      })\r\n    }\r\n\r\n    return { state, clickLogout, useStore, clickAdmin }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  h1 {\r\n    font-family: SangSangShinb7;\r\n    font-style: normal;\r\n    font-weight: normal;\r\n    font-size: 48px;\r\n    line-height: 4rem;\r\n    color: #26730B;\r\n    margin: 0;\r\n    margin-left: 2rem;\r\n    text-align: center;\r\n  }\r\n</style>"]}]}